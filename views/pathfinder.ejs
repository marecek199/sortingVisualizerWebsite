



    <%- include('partials/path_header') %> 


    <!-- Pathfinder / Snake -->
    <% if (header_name!='sort_header' ) { %>
        <div>
            <div class="legend_colors">
                <div class="legend_colors_item" style="justify-self: right; margin-right: 50px;"> Start point
                    <div class="rectangle_start"></div>
                </div>
                <div class="legend_colors_item" style="margin-left: 50px;"> End point
                    <div class="rectangle_end"></div>
                </div>
            </div>
        </div>   

        <div style="margin-left: 20%; margin-right: 20%;">
            <div class="<%= cointainer_name %>">
                <% for(let r=0; r< max_row; r++) {%>
                    <% for(let c=0; c < max_col; c++) {%>
                        <div class="grid-item"> </div>
                    <% } %> 
                <% } %> 
            </div>
        </div>

    <!-- Sorting Algorithm -->
    <% } else if (header_name=='sort_header' ) { %>
        <div style="margin-left: 5%; margin-right: 5%;">
            <div class="<%= cointainer_name %>">
                <% for(let r=0; r< max_row; r++) {%>
                    <% for(let c=0; c < 2*max_col; c++) {%>
                        <div class="grid-item_sort"> </div>
                    <% } %>
                <% } %>
            </div>
        </div>
    <% } %> 


    <%- include('partials/path_footer') %> 
    




 